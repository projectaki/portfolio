{
  "name": "portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.component.ts",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "format": "npx prettier src/**/*.{ts,html,css,scss} --write",
    "lint": "npx eslint src/**/*.{ts,html} --fix",
    "format-lint": "npm run format && npm run lint",
    "prepare": "husky install",
    "deploy": "ng deploy",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "scully": "npx scully --",
    "scully:serve": "npx scully serve --",
    "scully:build": "ng build --stats-json && npx scully && npx scully serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.2",
    "@angular/cdk": "^13.1.2",
    "@angular/common": "~13.1.2",
    "@angular/compiler": "~13.1.2",
    "@angular/core": "~13.1.2",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/forms": "~13.1.2",
    "@angular/material": "^13.1.2",
    "@angular/platform-browser": "~13.1.2",
    "@angular/platform-browser-dynamic": "~13.1.2",
    "@angular/router": "~13.1.2",
    "@scullyio/init": "^2.1.9",
    "@scullyio/ng-lib": "^2.1.0",
    "@scullyio/scully": "^2.1.0",
    "@scullyio/scully-plugin-puppeteer": "^2.1.0",
    "ngx-markdown": "^13.0.0",
    "primeicons": "^5.0.0",
    "primeng": "^13.0.3",
    "rxjs": "~7.5.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.3",
    "@angular-eslint/builder": "12.7.0",
    "@angular-eslint/eslint-plugin": "12.7.0",
    "@angular-eslint/eslint-plugin-template": "12.7.0",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "12.7.0",
    "@angular/cli": "~13.1.3",
    "@angular/compiler-cli": "~13.1.2",
    "@cypress/schematic": "^1.6.0",
    "@scullyio/scully-plugin-copy-to-clipboard": "^2.1.9",
    "@types/jasmine": "~3.8.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "angular-cli-ghpages": "^1.0.0",
    "cypress": "latest",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jasmine-core": "~3.8.0",
    "jest": "^27.4.7",
    "jest-preset-angular": "^11.0.1",
    "karma-coverage": "~2.0.3",
    "lint-staged": "^12.3.2",
    "prettier": "^2.5.1",
    "scully-plugin-disable-angular": "^8.0.0",
    "typescript": "~4.5.4"
  }
}
